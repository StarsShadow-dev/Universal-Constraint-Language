makeIdentity = @T(Type) @x(T) x

makeIdentity String
makeIdentity String "Hello, World!"
makeIdentity Number
makeIdentity Number 81
makeIdentity Function
makeIdentity Function (@x(String) x)
makeIdentity Function (@x(String) x) "Bye, World?"